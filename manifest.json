{
    "manifest_version": 2,
    "name": "YouTube Endcard Toggle",
    "version": "1.0",
    "description": "Toggle YouTube endcards on/off",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  